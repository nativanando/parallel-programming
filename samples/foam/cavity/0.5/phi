FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0.5";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];
oriented        oriented;


internalField   uniform 0;

boundaryField
{
    wall-geom_solid
    {
        type            empty;
        value           nonuniform 0();
    }
}


